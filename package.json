{
  "name": "app-template-reason-react",
  "version": "3.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jihchi/app-template-reason-react.git"
  },
  "keywords": [
    "csa-template",
    "reason-react",
    "rescript-react",
    "reasonml",
    "bucklescript",
    "rescript",
    "react"
  ],
  "files": [
    ".gitignore",
    "bsconfig.json",
    "README.md",
    "snowpack.config.js",
    "web-test-runner.config.js",
    "public/*",
    "src/**/*.res",
    "src/**/*.css",
    "src/**/*.svg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:res && snowpack build",
    "build:res": "rescript",
    "clean": "npm run clean:res",
    "clean:res": "rescript clean",
    "start": "snowpack dev",
    "test": "web-test-runner \"src/**/__tests__/*.bs.js\""
  },
  "dependencies": {
    "@rescript/react": "^0.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.4.2",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.6",
    "@web/test-runner": "^0.12.19",
    "chai": "^4.3.4",
    "rescript": "^9.1.1",
    "snowpack": "^3.3.3"
  }
}
